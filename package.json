{
  "name": "@mutantlove/pluginus",
  "version": "0.7.0",
  "description": "Dependency injection with promise support.",
  "homepage": "https://github.com/mutantlove/pluginus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/pluginus.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/pluginus/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "dist/pluginus.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----BUNDLE": "",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "build:ci": "babel src --out-dir dist",
    "----LINT": "",
    "lint:js": "eslint --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "tdd": "nodemon --watch src --exec \"npm test\"",
    "test": "tape -r @babel/register src/*.test.js | tap-nirvana",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm run test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "dependency-injection",
    "plugin-manager",
    "node"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@mutantlove/m": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/register": "7.7.7",
    "@mutantlove/eslint-config": "5.5.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-no-inferred-method-name": "2.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "15.0.1",
    "glob": "7.1.6",
    "markdownlint-cli": "0.21.0",
    "nodemon": "2.0.2",
    "npm-check": "5.9.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "semantic-release": "16.0.1",
    "tap-nirvana": "1.1.0",
    "tape": "4.13.0"
  }
}
