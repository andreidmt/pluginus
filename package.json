{
  "name": "@asd14/pluginus",
  "version": "0.6.2",
  "description": "Dependency injection with promise support.Things that get ran after other things.",
  "homepage": "https://github.com/asd14/pluginus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd14/pluginus.git"
  },
  "bugs": {
    "url": "https://github.com/asd14/pluginus/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "lib/pluginus.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUILD------------------------------------": "",
    "build": "babel src -d lib",
    "prepare": "npm audit fix && npm test && npm run lint && npm run build",
    "prepublishOnly": "npm run coverage",
    "----LINT-------------------------------------": "",
    "lint:js": "eslint --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST-------------------------------------": "",
    "test": "tape -r @babel/register src/**/*.test.js",
    "tdd": "DEBUG=Pluginus:* nodemon --watch src --watch examples --exec \"npm run test | tap-diff\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "dependency-injection",
    "plugin-manager",
    "node"
  ],
  "dependencies": {
    "@asd14/m": "0.20.0",
    "@babel/runtime": "7.4.3"
  },
  "devDependencies": {
    "@asd14/eslint-config": "4.0.2",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/register": "7.4.0",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "8.0.2",
    "glob": "7.1.3",
    "markdownlint-cli": "0.15.0",
    "nodemon": "1.18.11",
    "npm-check": "5.9.0",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "tap-diff": "0.1.1",
    "tape": "4.10.1"
  }
}
