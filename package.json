{
  "name": "@codemachiner/pluginus",
  "version": "0.1.0",
  "description": "Async dependency plugin container",
  "homepage": "https://github.com/codemachiner/pluginus",
  "main": "src/pluginus.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemachiner/pluginus.git"
  },
  "bugs": {
    "url": "https://github.com/codemachiner/pluginus/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "email": "andrei@codemachiner.com"
  },
  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i",
    "check:update": "npm-check -u",
    "check:security": "nsp check",
    "----BUNDLE-----------------------------------": "",
    "prepublish": "npm test && npm run lint && npm run check:security",
    "----LINTING----------------------------------": "",
    "lint": "eslint --fix ./src",
    "----TESTS------------------------------------": "",
    "test": "tape src/**/*.test.js | tap-diff",
    "tdd": "DEBUG=Pluginus* nodemon --watch src --exec \"npm test\""
  },
  "dependencies": {
    "@codemachiner/m": "0.3.0",
    "blue-tape": "1.0.0",
    "debug": "3.1.0",
    "tap-diff": "0.1.1"
  },
  "devDependencies": {
    "@codemachiner/eslint-config": "2.9.6",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-unicorn": "4.0.3",
    "markdownlint-cli": "0.8.1",
    "nodemon": "1.17.3",
    "npm-check": "5.7.0",
    "nsp": "3.2.1"
  },
  "license": "MIT"
}
